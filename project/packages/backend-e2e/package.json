{
  "name": "@project/backend-e2e",
  "version": "0.0.1",
  "private": true,
  "nx": {
    "implicitDependencies": [
      "@project/backend"
    ],
    "targets": {
      "e2e": {
        "executor": "@nx/jest:jest",
        "outputs": [
          "{projectRoot}/test-output/jest/coverage"
        ],
        "options": {
          "jestConfig": "packages/backend-e2e/jest.config.ts",
          "passWithNoTests": true
        },
        "dependsOn": [
          "@project/backend:build",
          "@project/backend:serve"
        ]
      }
    }
  },
  "devDependencies": {
    "@swc/core": "~1.5.29",
    "@swc/jest": "^0.2.39"
  }
}
